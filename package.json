{
	"name": "@mhingston5/maestro",
	"version": "0.1.0",
	"description": "Write declarative workflows in YAML, run them with Mastra",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"maestro": "dist/cli.js"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"mastra",
		"workflow",
		"yaml",
		"orchestrator",
		"ai",
		"agent"
	],
	"author": "mhingston5",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mhingston5/maestro.git"
	},
	"bugs": {
		"url": "https://github.com/mhingston5/maestro/issues"
	},
	"homepage": "https://github.com/mhingston5/maestro#readme",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"format": "biome format .",
		"lint": "biome lint .",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest run"
	},
	"dependencies": {
		"@mastra/core": "^1.1.0",
		"@mastra/schema-compat": "^1.0.0",
		"fast-glob": "^3.3.0",
		"gray-matter": "^4.0.3",
		"yaml": "^2.5.0",
		"zod": "^3.25.0",
		"zod-to-json-schema": "^3.23.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@vitest/coverage-v8": "^2.1.0",
		"typescript": "^5.5.0",
		"vitest": "^2.1.0"
	}
}
